    "scripts": {
        "start": "cross-env REACT_APP_STAGE=dev node --max-old-space-size=5120 scripts/start.js",
        "start:qa": "cross-env REACT_APP_STAGE=qa node scripts/start.js",
        "start:uat": "cross-env REACT_APP_STAGE=uat node scripts/start.js",
        "start:vpen": "cross-env REACT_APP_STAGE=vpen node scripts/start.js",
        "local": "cross-env REACT_APP_STAGE=local node scripts/start.js",
        "dev": "cross-env REACT_APP_STAGE=dev node scripts/start.js",
        "build:dev": "cross-env REACT_APP_STAGE=dev node --max-old-space-size=5120 scripts/build.js",
        "build:qa": "cross-env REACT_APP_STAGE=qa node --max-old-space-size=5120 scripts/build.js",
        "build:uat": "cross-env REACT_APP_STAGE=uat node --max-old-space-size=5120 scripts/build.js",
        "build:vpen": "cross-env REACT_APP_STAGE=vpen node --max-old-space-size=5120 scripts/build.js",
        "test": "node scripts/test.js",
        "lint": "eslint .",
        "check-staged": "lint-staged",
        "start-int": "cross-env REACT_APP_STAGE=int node scripts/start.js",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
        "navcli": "node templateGenerator/index.js"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint"
        ],
        "*.{js,jsx,json,md}": [
            "prettier --list-different"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "devDependencies": {
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react-hooks": "^1.6.1",
        "husky": "^3.0.8",
        "lint-staged": "^9.2.0",
        "prettier": "1.18.2",
        "redux-logger": "^3.0.6",
        "uglifyjs-webpack-plugin": "^2.2.0"
    }
}
